{
  "name": "browser-dynamics",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "wasm-pack build crates/wasm --release --target web --out-dir pkg",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run && cargo test",
    "clean": "rm -rf dist/ && cargo clean && cd crates/wasm && rm -rf pkg",
    "cli": "cargo run --release --bin sph-cli -- --width 400 --height 300 -f 60 -s 10 -o animation",
    "container": "./start-claude-container.sh"
  },
  "devDependencies": {
    "@types/three": "^0.180.0",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "~5.9.3",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "three": "^0.180.0"
  }
}
